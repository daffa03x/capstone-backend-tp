<table id="example2" class="table table-bordered table-striped text-center">
    <thead>
    <tr>
        <th>No</th>
        <th>Nama</th>
        <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
        <% for(let i = 0;i<category.length;i++){ %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= category[i].name %></td>
                <td>
                    <form action="/category/delete/<%=category[i]._id%>?_method=DELETE" method="POST">
                        <a href="/category/edit/<%=category[i]._id%>" class="btn btn-warning text-white"><i class="nav-icon fas fa-pencil-alt"></i>&ensp;Ubah</a>
                        <button class="btn btn-danger"><i class="nav-icon fas fa-trash"></i>&ensp;Hapus</button>
                    </form>
                </td>
            </tr>
        <% } %>
    </tbody>
    </table>